    <body>
        <div class="container">
            <div class="row">
                <div class="col-md-10 col-md-offset-1 text-center">
                      <h2>Searcher</h2>
                </div>
            </div>
            <div class="row">
                <div class="col-md-10 col-md-offset-1">
                    <div id="imaginary_container">
                        <div class="input-group stylish-input-group searcher">
                            <input class="searcherText form-control" type="search" id="searchText" placeholder="Search..." />
                            <span class="input-group-addon">
                                <button class="btn-static" type="submit">
                                    <span class="glyphicon glyphicon-search"></span>
                                </button>
                            </span>
                        </div>
                    </div>

                    <div class="form-group result">
                        <textarea class="form-control" id="result" rows="3"></textarea>
                    </div>
                </div>
            </div>
        </div>

